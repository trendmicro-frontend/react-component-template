{
  "name": "@trendmicro/react-component",
  "version": "0.1.0",
  "description": "React Component",
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "prepublish": "npm run lint && npm test && npm run clean && npm run build && npm run styleguide:build",
    "build": "webpack-cli && npm run cleancss",
    "clean": "rm -f {lib,dist}/*",
    "cleancss": "cleancss -o dist/react-component.min.css dist/react-component.css",
    "demo": "http-server -p 8000 docs/",
    "lint": "npm run eslint && npm run stylint",
    "eslint": "eslint --ext .js --ext .jsx *.js src test",
    "stylint": "stylint src",
    "test": "tap test/*.js --node-arg=--require --node-arg=@babel/register --node-arg=--require --node-arg=@babel/polyfill",
    "coveralls": "tap test/*.js --coverage --coverage-report=text-lcov --nyc-arg=--require --nyc-arg=@babel/register --nyc-arg=--require --nyc-arg=@babel/polyfill | coveralls",
    "dev": "npm run styleguide",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trendmicro-frontend/react-component.git"
  },
  "author": "",
  "contributors": [
    {
      "name": "",
      "email": "",
      "url": "https://github.com/trendmicro-frontend"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trendmicro-frontend/react-component/issues"
  },
  "homepage": "https://github.com/trendmicro-frontend/react-component",
  "keywords": [
    "react",
    "react-component"
  ],
  "peerDependencies": {
    "react": "^0.14.0 || >=15.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.1.2",
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "~7.0.0",
    "@babel/preset-env": "~7.1.0",
    "@babel/preset-react": "~7.0.0",
    "@babel/register": "~7.0.0",
    "@trendmicro/babel-config": "^1.0.0-alpha",
    "babel-eslint": "~10.0.1",
    "babel-loader": "~8.0.4",
    "clean-css": "~4.2.1",
    "clean-css-cli": "~4.2.1",
    "coveralls": "~3.0.2",
    "cross-env": "~5.2.0",
    "css-loader": "~1.0.0",
    "enzyme": "~3.7.0",
    "enzyme-adapter-react-16": "~1.6.0",
    "eslint": "~5.7.0",
    "eslint-config-trendmicro": "~1.4.1",
    "eslint-loader": "~2.1.1",
    "eslint-plugin-import": "~2.14.0",
    "eslint-plugin-jsx-a11y": "~6.1.2",
    "eslint-plugin-react": "~7.11.1",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "~2.0.0",
    "find-imports": "~1.0.0",
    "html-webpack-plugin": "~3.2.0",
    "http-server": "~0.11.1",
    "jsdom": "~12.2.0",
    "mini-css-extract-plugin": "~0.4.4",
    "nib": "~1.1.2",
    "react": "~16.6.0",
    "react-dom": "~16.6.0",
    "react-styleguidist": "~8.0.0",
    "style-loader": "~0.23.1",
    "stylint": "~1.5.9",
    "stylus-loader": "~3.0.2",
    "tap": "~12.0.1",
    "trendmicro-ui": "~0.5.2",
    "url-loader": "~1.1.2",
    "webpack": "~4.23.0",
    "webpack-cli": "~3.1.2",
    "webpack-dev-server": "~3.1.10"
  }
}
